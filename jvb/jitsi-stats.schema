CREATE TABLE IF NOT EXISTS conferences (
    id INTEGER PRIMARY_KEY,
    conference_name TEXT,
    conference_id TEXT,
    start TEXT,
    end TEXT
);
CREATE TABLE IF NOT EXISTS state (
    id INTEGER PRIMARY_KEY,
    time TEXT,
    filename TEXT,
    filetime INTEGER,
    position INTEGER CHECK(typeof(position)='integer'),
    inode INTEGER
);
